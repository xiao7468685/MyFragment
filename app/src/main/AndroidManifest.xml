        <activity android:parentActivityName=".StartActivity"
            android:name=".MyActionBarActivity"
            android:label="ActionBar"
            android:logo="@drawable/pedometer">

        </activity>
        <activity android:name=".StartActivity"
            android:logo="@drawable/ic_action_call"
            android:label="计步器"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
